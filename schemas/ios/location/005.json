{
    "$schema": "http://json-schema.org/schema#",
    "id": "https://schemas.datacubed.com/ios/005/location.json",
    "type": "array",
    "items":    {
        "type": "object",
        "properties": {
            "latitude": {
                "type": "number",
                "minimum": -90,
                "maximum": 90
            },
             "bearing": {
                "type": "number"
            },
             "altitude": {
                "type": "number"
            },
             "altitude_accuracy": {
                "type": "number"
            },
            "speed": {
                "type": "number"
            },
            "longitude": {
                "type": "number",
                "minimum": -180,
                "maximum": 180
            },
            "timestamp": {
               "$ref": "https://schemas.datacubed.com/definitions.json#/timestamp_def"

            },
            "accuracy": {
                "type": "number",
                "minimum": 0
            },
            "provider": {
                "type": "string"
            }
        },
        "required": ["latitude","longitude","timestamp","accuracy","provider"]
    },
    "minItems": 1
}
