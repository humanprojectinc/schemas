{
    "$schema": "http://json-schema.org/schema#",
    "id": "https://schemas.datacubed.com/ios/006/survey.json",
    "items": {
        "properties": {
            "completion": {
                "$ref": "https://schemas.datacubed.com/definitions.json#/timestamp_def"
            },
            "gamification_points": {
                "type": "number"
            },
            "identity": {
                "type": "string"
            },
            "questions": {
                "items": {
                    "properties": {
                        "answer": {
                            "items": {
                                "properties": {
                                    "label": {
                                        "type": "string"
                                    },
                                    "value": {
                                        "type": "string"
                                    }
                                },
                                "type": "object"
                            },
                            "type": "array"
                        },
                        "id": {
                            "type": "string"
                        },
                        "state": {
                            "type": "string"
                        }
                    },
                    "required": [
                        "answer",
                        "id",
                        "state"
                    ],
                    "type": "object"
                },
                "type": "array"
            },
            "start": {
                "$ref": "https://schemas.datacubed.com/definitions.json#/timestamp_def"
            },
            "state": {
                "type": "string"
            }
        },
        "required": [
            "completion",
            "gamification_points",
            "identity",
            "questions",
            "start",
            "state"
        ],
        "type": "object"
    },
    "minItems": 1,
    "type": "array"
}
