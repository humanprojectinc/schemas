{
    "$schema": "http://json-schema.org/schema#",
    "id": "http://hpi.nyc/schemas/ios/001/gmail.json",
    "type": "array",
    "items":    {
        "type": "object",
        "properties": {
            "from": {
                "type": "string"
            },
            "to": {
                "type": "string"
            },
            "identifier": {
                "type": "number",
                "minimum": 0
            },
            "threadId": {
                "type": "number",
                "minimum": 0
            },
            "internalDate": {
                "type": "string"
	        },
            "labels": {
                "type": "array",
                "items": {
                    "type": "string"
                }
            }
        },
        "required": ["from","to","identifier","threadId","internalDate","labels"]
    }
}