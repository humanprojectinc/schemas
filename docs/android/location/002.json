{
    "$schema": "http://json-schema.org/schema#",
    "id": "https://schemas.datacubed.com/android/location/002.json",
    "index": "https://schemas.datacubed.com/index.json",
    "type": "array",
    "items":    {
        "type": "object",
        "properties": {
            "latitude": {
                "type": "number",
                "minimum": -90,
                "maximum": 90
            },
            "longitude": {
                "type": "number",
                "minimum": -180,
                "maximum": 180
            },
            "accuracy": {
                "type": "number",
                "minimum": 0
            },
            "provider": {
                "type": "string"
            },
            "speed": {
                "type": "string"
            },
             "bearing": {
                "type": "string"
            },
             "altitude": {
                "type": "string"
            },
            "timestamp": {
                "$ref": "https://schemas.datacubed.com/definitions.json#/timestamp_def"
            }
        },
        "required": ["longitude","latitude","accuracy","timestamp"]
    }
}
