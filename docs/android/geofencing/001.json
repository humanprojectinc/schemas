{
    "$schema": "http://json-schema.org/schema#",
    "id": "https://schemas.datacubed.com/android/geofencing/001.json",
    "index": "https://schemas.datacubed.com/index.json",
    "type": "array",
    "items":    {
        "type": "object",
        "properties": {
          "geofenceName": {
            "type": "string"
          },
          "locationLatitude": {
              "type": "number",
              "minimum": -90,
              "maximum": 90
          },
          "locationLongitude": {
              "type": "number",
              "minimum": -180,
              "maximum": 180
          },
          "locationAccuracy": {
              "type": "number",
              "minimum": 0
          },
          "timestamp": {
              "$ref": "https://schemas.datacubed.com/definitions.json#/timestamp_def"
          },
          "geofenceLatitude": {
              "type": "number",
              "minimum": -90,
              "maximum": 90
          },
          "geofenceLongitude": {
              "type": "number",
              "minimum": -180,
              "maximum": 180
          },
          "eventType": {
            "type": "string"
          },
          "geofenceRadius": {
              "type": "number",
              "minimum": 0
          }
        },
        "required": ["geofenceName", "locationLatitude", "locationLongitude", "timestamp", "eventType", "geofenceRadius", "geofenceLatitude", "geofenceLongitude", "locationAccuracy"]
    }
}
