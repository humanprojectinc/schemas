{
    "$schema": "http://json-schema.org/schema#",
    "id": "https://schemas.datacubed.com/android/geofencing/001.json",
    "index": "https://schemas.datacubed.com/index.json",
    "type": "array",
    "items":    {
        "type": "object",
        "properties": {
          "geofence_name": {
            "type": "string"
          },
          "location_latitude": {
              "type": "number",
              "minimum": -90,
              "maximum": 90
          },
          "location_longitude": {
              "type": "number",
              "minimum": -180,
              "maximum": 180
          },
          "location_accuracy": {
              "type": "number",
              "minimum": 0
          },
          "timestamp": {
              "$ref": "https://schemas.datacubed.com/definitions.json#/timestamp_def"
          },
          "geofence_latitude": {
              "type": "number",
              "minimum": -90,
              "maximum": 90
          },
          "geofence_longitude": {
              "type": "number",
              "minimum": -180,
              "maximum": 180
          },
          "event_type": {
            "type": "string"
          },
          "geofence_radius": {
              "type": "number",
              "minimum": 0
          },
        },
        "required": ["geofence_name", "location_latitude", "location_longitude", "timestamp", "event_type", "geofence_radius", "geofence_latitude", "geofence_longitude", "location_accuracy"]
    }
}
