{
    "$schema": "http://json-schema.org/schema#",
    "id": "https://schemas.datacubed.com/android/002/task.json",
    "index": "https://schemas.datacubed.com/index.json",
    "type": "array",
    "items":    {
        "type": "object",
        "properties": {
            "identity": {
                "type": "string"
            },
            "last_updated": {
              "$ref": "https://schemas.datacubed.com/definitions.json#/timestamp_def"
            },
            "title": {
              "type": "string"
            },
            "display_icon": {
              "type": "string"
            },
            "est_duration": {
              "type": "string"
            },
            "gamification_points": {
                "type": "number"
            },
            "version": {
                "type": "number"
            },
            "attribution_text": {
              "type": "string"
            },
            "data": {
                "type": "object",
                "properties": {
                  "data": {
                    "type": "object",
                    "properties": {
                      "start": {
                        "$ref": "https://schemas.datacubed.com/definitions.json#/timestamp_def"
                      }
                    }
                  },
                  "end":{
                    "$ref": "https://schemas.datacubed.com/definitions.json#/timestamp_def"
                  },
                  "module": {
                    "type": "string"
                  },
                  "start":{
                    "$ref": "https://schemas.datacubed.com/definitions.json#/timestamp_def"
                  },
                  "variant": {
                    "type": "string"
                  }
                },
                "required": ["data", "start", "end"]
            }
        },
        "required": ["data", "identity", "gamification_points","last_updated","title","display_icon","est_duration","version"]
    },
    "minItems": 1
}
