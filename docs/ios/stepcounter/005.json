{
    "$schema": "http://json-schema.org/schema#",
    "id": "https://schemas.datacubed.com/ios/005/stepcounter.json",
    "type": "array",
    "items":    {
        "type": "object",
        "properties": {
            "steps": {
                "type": "number"
            },
            "starttime": {
                "$ref": "https://schemas.datacubed.com/definitions.json#/timestamp_def"
            },
            "endtime": {
                "$ref": "https://schemas.datacubed.com/definitions.json#/timestamp_def"
            }
        },
        "required": ["steps", "starttime", "endtime"]
    },
    "minItems": 1
}
